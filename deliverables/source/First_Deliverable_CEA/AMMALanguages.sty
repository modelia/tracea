        sensitive=true,
        morecomment=[l]{--},
        morestring=[b]',
}

\lstdefinelanguage{USE}{
        morekeywords={context, inv, body, if, then, else, endif, post, pre, class, end, begin, attributes, operations, composition, association, aggregarion, let, in, enum, derive, init, contraints},
        emph={Boolean,Integer,String,Real, Sequence, Bag, self, Set, UReal, UBoolean, UString, Tuple, result, ordered, and, or, implies, xor, not, reject, includes, including, includesAll, size, select, collect, any, one },
        sensitive=true,
        morecomment=[l]{--},
        morestring=[b]',
}
\lstdefinelanguage{Esper}{
        morekeywords={insert, into, select, as, from, pattern, group, by, where, every, timer, at, win, time_batch, length_batch, within, minutes, seconds, hours, having, ->, @Name},
        morekeywords={not, and, or, min, max, true, false},
